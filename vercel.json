{
    "functions": {
      "api/proxy.js": {
        "memory": 1000,
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/api/proxy(?:/.*)?",
        "dest": "/api/proxy.js"
      },
      {
        "src": "/rails/active_storage/blobs/redirect(?:/.*)?",
        "dest": "/api/proxy.js"
      }
    ]
  }
  
